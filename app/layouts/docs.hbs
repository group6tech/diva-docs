---
layout: structure.hbs
---

<div class="offcanvas-wrap">
	<div class="offcanvas-inner">

		<div class="offcanvas-sidenav-left sidenav">
			{{> sidenav this }}
		</div>

		<div class="offcanvas-content">
			<a href="/docs/{{ version }}/index.html"><i class="fa fa-arrow-circle-left"></i> Back to documentation</a>

			<div class="content-wrapper">
				<h1 class="page-header">{{ title }}</h1>

				<div class="row">
					<div class="col-lg-push-9 col-lg-3">
						{{#if topics}}
							{{> toc }}
						{{/if}}
					</div>
					<div class="col-lg-pull-3 col-lg-9">
						{{> body }}
					</div>
				</div>
			</div>

			<a href="/docs/{{ version }}/index.html"><i class="fa fa-arrow-circle-left"></i>  Back to documentation</a>
		</br>
			<a href="#top"><i class="fa fa-arrow-circle-up"></i>  Back to top</a>
		</div>

	</div>
</div>
